@model List<KetQuaTimKiemVM>

<table class="table table-hover">
	<thead>
		<tr>
			<th>Mã</th>
			<th>Tên hàng hóa</th>
			<th>Đơn giá</th>
			<th>Ngày sản xuất</th>
			<th>Loại</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.MaHh</td>
				<td>@item.TenHh</td>
				<td>@item.DonGia.ToString("#,##0.00") đ</td>
				<td>@item.NgaySX.ToString("dd/MM/yyyy")</td>
				<td>@item.Loai</td>
			</tr>
		}
	</tbody>
</table>
